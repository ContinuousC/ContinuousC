  "itemTypes": {
    "kubernetes/pod": {
      "metrics": {
        "cpu_usage": {
          "name": "Average CPU Usage Over 5min",
          "documentation": "This metric represents the cumulative CPU time consumed in seconds.",
          "item": "kubelet-metrics-cadvisor:podCpu",
          "expr": "rate(container_cpu_usage_seconds_total[5m])",
          "thresholds": {
            "pod-cpu-usage": "$min"
          },
          "unit": "",
          "display_unit": ""
        },
        "cpu_throttled": {
          "name": "CPU throttled percentage Over 5min",
          "documentation": "The percentage of CPU periods where the item ran but was throttled (stopped from running the whole CPU period).",
          "item": "kubelet-metrics-cadvisor:podOtherMetrics",
          "expr": "rate(container_cpu_cfs_throttled_periods_total[5m])/rate(container_cpu_cfs_periods_total[5m])",
          "unit": "",
          "display_unit": "%"
        },
        "memory_rss": {
          "name": "Memory RSS",
          "documentation": "The amount of anonymous and swap cache memory (includes transparent hugepages).",
          "item": "kubelet-metrics-cadvisor:podOtherMetrics",
          "expr": "container_memory_rss",
          "unit": "B"
        },
        "memory_working_set": {
          "name": "Memory Working set",
          "documentation": "The amount of working set memory, this includes recently accessed memory,dirty memory, and kernel memory. Working set is <= 'usage'.",
          "item": "kubelet-metrics-cadvisor:podOtherMetrics",
          "expr": "container_memory_working_set_bytes",
          "thresholds": {
            "pod-memory-usage": "$min"
          },
          "unit": "B"
        },
        "network_receive": {
          "name": "Receive Bytes",
          "documentation": "Cumulative number of bytes received.",
          "item": "kubelet-metrics-cadvisor:pauseContainerInterface",
          "expr": "rate(container_network_receive_bytes_total[5m])",
          "unit": "B"
        },
        "network_transmit": {
          "name": "Transmitted Bytes",
          "documentation": "Cumulative number of bytes transmitted.",
          "item": "kubelet-metrics-cadvisor:pauseContainerInterface",
          "expr": "rate(container_network_transmit_bytes_total[5m])",
          "unit": "B"
        },
        "fs_read": {
          "name": "File system Read Bytes",
          "documentation": "Cumulative number of bytes read from the filesystem.",
          "item": "kubelet-metrics-cadvisor:containerIo",
          "expr": "rate(container_fs_reads_bytes_total[5m])",
          "select": {
            "top": {
              "n": 5
            }
          },
          "unit": "B"
        },
        "fs_write": {
          "name": "File system Write Bytes",
          "documentation": "Cumulative number of bytes written to the filesystem.",
          "item": "kubelet-metrics-cadvisor:containerIo",
          "expr": "rate(container_fs_writes_bytes_total[5m])",
          "unit": "B"
        }
      },
      "dashboards": {
        "type_range": {
          "panels": [
            {
              "name": "CPU",
              "widgets": [
                {
                  "name": "Average Usage Over 5min",
                  "documentation": "This metric represents the cumulative CPU time consumed in seconds.",
                  "widget_type": "lines",
                  "metric": "cpu_usage",
                  "select": {
                    "top": {
                      "n": 5
                    }
                  }
                },
                {
                  "name": "Throttled percentage Over 5min",
